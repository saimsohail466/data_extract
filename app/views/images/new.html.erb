<div class="container w-100 m-4 p-5">
  <div class="card">
    <div class="card-header">
      <h4> New Image </h4>
    </div>

    <div class="card-body">
      <%= form_with(model: [@image]) do |f| %>
        <%= f.label :image %>
        <%= f.file_field :image %>
        <%= f.submit class: 'btn btn-sm btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
